{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://eatnfit.app/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.eatnfit.app"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}

